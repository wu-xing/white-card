<ul class="article-list">
    <% articles.forEach(function(article){ %>
        <li>
          <h2><a href="<%= article.outputUrl %>"><%= article.title %></a></h2>
          <span class="show-time"><%- article.showTime %></span>
          <div class="hr"></div>
          <p><%- article.contentPart %></p>
        </li>
        
    <% }); %>
    
</ul>
<ul class="paging">
    <% for(var i = 0; i < pageN; i++) { %>
        <% var href = i === 0 ? '/' : '/page/' + i %>
        <% if(i === currentPageN) { %>
            <li class="paging--number active"><a href="<%= href %>"><%= i %></a></li>
        <% } else { %>
            <li class="paging--number%>"><a href="<%= href %>"><%= i %></a></li>
        <% } %>
    <% } %>
</ul>
